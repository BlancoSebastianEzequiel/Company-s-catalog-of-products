{
  "name": "server",
  "version": "0.0.1",
  "description": "Server del TP de ACPI I 2019/1",
  "main": "server",
  "engines": {
    "node": "^8.9.4"
  },
  "scripts": {
    "client": "cd client && npm run start",
    "server": "sh scripts/wsgi.sh",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server",
    "install": "sh scripts/install.sh",
    "beforetest": "eslint ./client",
    "test": "npm run beforetest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nodemon": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp-sourcemaps": "1.7.x",
    "nyc": "^13.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/BlancoSebastianEzequiel/Company-s-catalog-of-products.git"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/node_modules/**",
      "server/*",
      "scripts/**"
    ]
  }
}
